  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "bin",
      "program": "${workspaceFolder}/bin/benchart",
      "args": [
        "-v", "test/results.json",
        "-d", "test/output", "-n", "timeline"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "program": "${workspaceFolder}/test/plotter",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}